{
  "rewrites": [
    {
      "source": "/openai/:match*",
      "destination": "https://api.openai.com/:match*"
    },
    {
      "source": "/claude/:match*",
      "destination": "https://api.anthropic.com/:match*"
    },
    {
      "source": "/gemini/:match*",
      "destination": "https://generativelanguage.googleapis.com/:match*"
    },
    {
      "source": "/xai/:match*",
      "destination": "https://api.x.ai/:match*"
    },
    {
      "source": "/groq/:match*",
      "destination": "https://api.groq.com/openai/:match*"
    },
    {
      "source": "/pplx/v1/:match*",
      "destination": "https://api.perplexity.ai/:match*"
    },
    {
      "source": "/openrouter/:match*",
      "destination": "https://openrouter.ai/api/:match*"
    },
    {
      "source": "/upstage/v1/:match*",
      "destination": "https://api.upstage.ai/v1/solar/:match*"
    },
    {
      "source": "/meowchan/:match*",
      "destination": "https://api.meowchan.cn/:match*"
    }
  ]
}